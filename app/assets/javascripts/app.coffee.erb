app = angular.module("tzApp",[
  "templates"
  "ngRoute"
  "ngResource"
  "tzControllers"
])

app.config([ "$routeProvider",
  ($routeProvider)->
    $routeProvider
      .when("/",
        templateUrl: "<%= asset_path 'templates/search_time_zone.html' %>"
        controller: "SearchTimeZoneCtrl"
      )
])
